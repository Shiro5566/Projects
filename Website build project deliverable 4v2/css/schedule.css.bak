html, body{
	margin: 0;
	padding: 0;
	background-color: black;
}

h1{
	color: black;
	font-size: 3em;
	font-family: Merriweather;
}

p{
	color: white;
	font-size: 1.5em;
	font-family: Merriweather;
}

#frame{
	border-style: none;
	border-color: red;
	min-width: 1900px;
    background-color: lightgrey;
}

#nav-bar{
	border-style: none;
	border-color: blue; 
	height: 107px;
	background-color: black;
}

#logo{
	border-style: none;
	border-color: green;
	width: 100px;
	float: left;
}

#logo-img{
	height: 100px;
	width: 100px;
}

#nav-links{
	border-style: none;
	border-color: green;
	width: 81.5%;
	height: 105px;
	float: left;
}

#nav-link-centering{
	border-style: none;
	border-color: yellow;
	height: 100px;
	width: 1200px;
	margin: 0 16.5%;
	
}

.nav-menu{
	background-color: #2b2e63;
	width: 150px;
	height: 35px;
	float: left;
	text-align: center;
	margin: 30px 10%;
	padding-top: 5px;
}

.buttons-color {
	color: white;
	text-decoration: none;
	font-size: 1.5em;
	font-weight: bold;
	font-family: Merriweather;
}

.icon-links{
	border-style: none;
	border-color: green;
	width: 190px;
	height: 105px;
	float: right;
}

.icons{
	border-style: none;
	border-color: yellow;
	width: 30%;
	float: right;
	margin-top: 16%;
	text-align: center;
	padding-top: 5px;
}

.icons-size{
	height: 30px;
	width: 30px;
}

#header-background {
	border-style: none;
	border-color: blue;
	background-color: #57adbf;
}

#header{
	 border-style: none;
	border-color: green; 
	text-align: center;
	width: 65%;
	margin: auto;
	padding: 10px 35px;
}

#main-content{
	border-style: none;
	border-color: blue; 
	height: 524px;
	background-color: #798dc5;
}

.stream-schedules{
	border-style: none;
	border-color: cyan; 
	float: left;
	text-align: center;
	margin: 5% 12%;
	height: 300px;
	width: 24%;
}

.unordered-lists{
	list-style-type: none;
	font-size: 2em;
	padding-right: 35px;
	box-sizing: border-box;
	font-weight: bold;
	color: white;
}

#footer{
	border-style: none;
	border-color: blue;
	height: 107px;
	background-color: black;
}

#copyright{
	border-style: none;
	border-color: green;
	text-align: center;
	float: left;
	padding-top: 10px;
	box-sizing: border-box;
	height: 107px;
	margin-left: 10%;
	color: white;
}