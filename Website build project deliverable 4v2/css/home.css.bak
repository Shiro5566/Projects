html, body{
	margin: 0;
	padding: 0;
}

h1{
	color: black;
	font-size: 3em;
	font-family: Merriweather;
}

p{
	color: white;
	font-size: 1.5em;
	font-family: Merriweather;
}

#frame{
	border-style: none;
	border-color: red;
	min-width: 1900px;
    background-color: lightgrey;
}

#nav-bar{
	border-style: none;
	border-color: blue; 
	height: 107px;
	background-color: black;
}

#logo{
	border-style: none;
	border-color: green;
	width: 100px;
	float: left;
}

#logo-img{
	height: 100px;
	width: 100px;
}

#nav-links{
	border-style: none;
	border-color: green;
	width: 81.5%;
	height: 105px;
	float: left;
}

#nav-link-centering{
	border-style: none;
	border-color: yellow;
	height: 100px;
	width: 1200px;
	margin: 0 16.5%;
	
}

.nav-menu{
	background-color: #2b2e63;
	width: 150px;
	height: 35px;
	float: left;
	text-align: center;
	margin: 30px 10%;
	padding-top: 5px;
}

.nav-menu a:hover{
	color: orange;
}

.buttons-color {
	color: white;
	text-decoration: none;
	font-size: 1.5em;
	font-weight: bold;
	font-family: Merriweather;
}

.icon-links{
	border-style: none;
	border-color: green;
	width: 190px;
	height: 105px;
	float: right;
}

.icons{
	border-style: none;
	border-color: yellow;
	width: 30%;
	float: right;
	margin-top: 16%;
	text-align: center;
	padding-top: 5px;
}

.icons-size{
	height: 30px;
	width: 30px;
}

#header-background {
	border-style: none;
	border-color: blue;
	background-color: #57adbf;
}

#header{
	 border-style: none;
	border-color: green; 
	text-align: center;
	width: 65%;
	margin: auto;
	padding: 10px 35px;
	
}

#main-content {
	border-style: none;
	border-color: blue;
	background-color: #798dc5;
	
}

.main{
	border-style: none;
	border-color: green;
	height: 285px;
}

#buttons{
	border-style: none;
	border-color: yellow;
	width: 41%;
	height: 220px;
	float: left;
	margin: 1.5% 1%;
}

#button-centering{
	border-style: none;
	border-color: purple;
	height: 220px;
	width: 800px;
	margin: auto;
}

.button-side {
	border-style: none;
	border-color: cyan;
	float: left;
	margin: 7% 12%;
	background-color: #2b2e63;
	height: 100px;
	width: 200px;
	text-align: center;
	padding-top: 35px;
	box-sizing: border-box;
}

#text-side {
	border-style: none;
	border-color: yellow;
	width: 41%;
	height: 220px;
	float: left;
	margin: 2% 1.5%;
	text-align: center;
	padding-top: 30px;
	box-sizing: border-box;
}

.video {
	border-style: none;
	border-color: yellow;
	width: 25%;
	float: right;
	height: 250px;
	margin: 0.8% 1%;
}

.video-size{
	width: 100%;
	height: 100%;
}

#social-media {
	border-style: none;
	border-color: blue;
	background-color: #57adbf;
}

#social-media-header {
	border-style: none;
	border-color: green; 
	text-align: center;
	width: 55%;
	margin: auto;
	padding: 10px 0;
}

#social-media-links-background{
	background-color: #798dc5;
}

#social-media-links{
	border-style: none;
	border-color: green;
	height: 250px;
	width: 72%;
	margin: auto;
}

.media-link{
	border-style: none;
	border-color: cyan;
	height: 100px;
	width: 125px;
	float: left;
	margin: 5% 12%;
	

	
}

.social-media-icons-size{
	height: 125px;
	width: 125px;
}

figure {
	margin: 0;
	
}

figcaption{
	text-align: center;
	color: black;
	font-size: 1.5em;
	
}

#footer{
	border-style: none;
	border-color: blue;
	height: 107px;
	background-color: black;
}

#copyright{
	border-style: none;
	border-color: green;
	text-align: center;
	float: left;
	padding-top: 10px;
	box-sizing: border-box;
	height: 107px;
	margin-left: 10%;
	color: white;
}